ONR_LISTEN=:3000
ONR_API_KEY=change-me
ONR_PROVIDERS_DIR=./config/providers
ONR_KEYS_FILE=./keys.yaml
ONR_MODELS_FILE=./models.yaml
ONR_MASTER_KEY=
ONR_READ_TIMEOUT_MS=60000
ONR_WRITE_TIMEOUT_MS=60000

# Usage estimation (best-effort). When enabled, missing/zero usage will be estimated.
ONR_USAGE_ESTIMATION_ENABLED=true

# Traffic dump (files)
ONR_TRAFFIC_DUMP_ENABLED=false
ONR_TRAFFIC_DUMP_DIR=./dumps
ONR_TRAFFIC_DUMP_FILE_PATH={{.request_id}}.log
ONR_TRAFFIC_DUMP_MAX_BYTES=1048576
ONR_TRAFFIC_DUMP_MASK_SECRETS=true

# Upstream key env override examples:
# ONR_UPSTREAM_KEY_OPENAI_KEY1=sk-xxxx
# ONR_UPSTREAM_KEY_ANTHROPIC_KEY1=sk-ant-xxxx
